{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Setup Script",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/Setup Script.mjs",
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "name": "Launch Seed Script",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/Seed Script.mjs",
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "name": "Attach to Node Process",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/Setup Script.mjs",
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env",
      "args": ["--inspect-brk=9229"],
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}